<view class="container data-v-34a496a9"><view class="header data-v-34a496a9"><cu-custom vue-id="1" bgColor="bg-blue" isBack="{{true}}" class="data-v-34a496a9" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText" class="data-v-34a496a9">返回</view><view slot="content" class="data-v-34a496a9">{{title}}</view></cu-custom></view><view class="main data-v-34a496a9"><scroll-view class="fill data-v-34a496a9" scroll-y="{{true}}"><form class="data-v-34a496a9"><view class="main-header data-v-34a496a9"><radio-group data-event-opts="{{[['change',[['handleTypeChange',['$event']]]]]}}" bindchange="__e" class="data-v-34a496a9"><radio color="#2db7f5" value="0" checked="{{formData.type==0}}" class="data-v-34a496a9">客户</radio><radio class="margin-left data-v-34a496a9" color="#2db7f5" value="1" checked="{{formData.type==1}}">供应商</radio><radio class="margin-left data-v-34a496a9" color="#2db7f5" value="2" checked="{{formData.type==2}}">所有</radio></radio-group></view><view class="cu-form-group data-v-34a496a9"><view class="title data-v-34a496a9">单位名称</view><input type="text" name="company" placeholder-class="text-disabled" placeholder="请输入单位名称" data-event-opts="{{[['input',[['__set_model',['$0','company','$event',[]],['formData']]]]]}}" value="{{formData.company}}" bindinput="__e" class="data-v-34a496a9"/></view><view class="cu-form-group data-v-34a496a9"><view class="title data-v-34a496a9">联系人名称</view><input type="text" name="contacts" placeholder-class="text-disabled" placeholder="请输入联系人名称" data-event-opts="{{[['input',[['__set_model',['$0','contacts','$event',[]],['formData']]]]]}}" value="{{formData.contacts}}" bindinput="__e" class="data-v-34a496a9"/></view><view class="cu-form-group data-v-34a496a9"><view class="title data-v-34a496a9">电话</view><input type="text" name="mobile" placeholder-class="text-disabled" placeholder="请输入电话" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['formData']]]]]}}" value="{{formData.mobile}}" bindinput="__e