<view class="container"><view class="header"><uni-navbar vue-id="1" title="{{title}}" left-icon="back" background-color="#2d8cf0" color="#fff" status-bar fixed data-event-opts="{{[['^clickLeft',[['handleNavbarClickLeft']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-navbar></view><view class="top"><view><text class="text">账户设置</text></view><view><button type="default" size="mini" data-event-opts="{{[['tap',[['handleAdd',['$event']]]]]}}" bindtap="__e">增加</button></view></view><view class="main"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item"><view class="list-between"><view>账户名称：<text>{{item.accountName}}</text></view></view><view class="list-between"><view class="list_bottom_box_item"><text>{{"账户类型："+item.bankType}}</text></view><view class="list_bottom_box_item"><text>{{"余额："+item.money}}</text></view></view></view></block><block wx:if="{{dataList.length===0}}"><view class="no_data"><text class="item_text">暂无数据</text></view></block></view></view>