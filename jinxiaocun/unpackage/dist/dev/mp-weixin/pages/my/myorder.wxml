<view class="container"><view class="header"><uni-navbar vue-id="1" title="{{title}}" left-icon="back" background-color="#2d8cf0" color="#fff" status-bar fixed data-event-opts="{{[['^clickLeft',[['handleNavbarClickLeft']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-navbar></view><view class="main"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item"><view class="list-between"><view>订单号：<text>{{item.orderNo}}</text></view><view><text>{{"¥"+item.money}}</text></view></view><view class="list-between"><text>订单状态</text><text class="{{['payment',item.paymentStatus=='待支付'?'payment-blue':'payment-green']}}">{{item.paymentStatus}}</text></view><view class="list-center"><text>{{item.description}}</text></view><view class="list-between"><view class="list_bottom_box_item"><text>{{"开始日期："+item.startDate}}</text></view><view class="list_bottom_box_item"><text>{{"到期日期："+item.expireDate}}</text></view></view></view></block><block wx:if="{{dataList.length===0}}"><view class="no_data"><text class="item_text">暂无数据</text></view></block></view></view>