<view class="container data-v-0d470ad4"><view class="header data-v-0d470ad4"><uni-navbar vue-id="1" title="{{title}}" left-icon="back" background-color="#2d8cf0" color="#fff" status-bar fixed data-event-opts="{{[['^clickLeft',[['handleNavbarClickLeft']]]]}}" bind:clickLeft="__e" class="data-v-0d470ad4" bind:__l="__l"></uni-navbar></view><view class="main data-v-0d470ad4"><scroll-view class="fill data-v-0d470ad4" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['loadData',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item data-v-0d470ad4"><view class="list-between data-v-0d470ad4"><view class="data-v-0d470ad4">订单号：<text style="font-weight:600;" class="data-v-0d470ad4">{{item.ordercode}}</text></view></view><view class="list-between data-v-0d470ad4"><view class="data-v-0d470ad4">商名名称：<text class="data-v-0d470ad4">{{item.ordertitle}}</text></view></view><view class="list-between data-v-0d470ad4"><view class="data-v-0d470ad4">订单金额：<text class="data-v-0d470ad4">{{"¥"+item.orderamount}}</text></view><text class="{{['payment data-v-0d470ad4',item.orderstatus==0?'payment-blue':'payment-green']}}">{{OrderStatusList[item.orderstatus]}}</text></view><view class="list-between data-v-0d470ad4"><view class="data-v-0d470ad4">付款类型：<text class="data-v-0d470ad4">{{PayTypeList[item.paytype]}}</text></view><view class="data-v-0d470ad4">购买时长：<text class="data-v-0d470ad4">{{item.daycount+"天"}}</text></view></view><view class="list-between data-v-0d470ad4"><view class="list_bottom_box_item data-v-0d470ad4"><text class="data-v-0d470ad4">{{"下单时间："+item.createtime}}</text></view></view><view class="list-between data-v-0d470ad4"><view class="list_bottom_box_item data-v-0d470ad4"><text class="data-v-0d470ad4">{{"付款时间："+(item.paysuccesstime||'')}}</text></view></view></view></block><block wx:if="{{dataList.length===0}}"><view class="no_data data-v-0d470ad4"><text class="item_text data-v-0d470ad4">暂无数据</text></view></block><block wx:if="{{dataList.length>=10}}"><uni-load-more vue-id="2" status="{{loadmore}}" class="data-v-0d470ad4" bind:__l="__l"></uni-load-more></block><cu-loading vue-id="3" data-ref="loading" class="data-v-0d470ad4 vue-ref" bind:__l="__l"></cu-loading></scroll-view></view></view>